<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <Button
        android:id="@+id/snake_is_near_me"
        android:layout_width="204dp"
        android:layout_height="178dp"
        android:layout_marginEnd="208dp"
        android:text="Snake has bitten me"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.256" />

    <Button
        android:id="@+id/forest_department"
        android:layout_width="204dp"
        android:layout_height="178dp"
        android:text="Snake is near me"
        app:layout_constraintBottom_toBottomOf="@+id/snake_is_near_me"
        app:layout_constraintStart_toEndOf="@+id/snake_is_near_me"
        app:layout_constraintTop_toTopOf="@+id/snake_is_near_me"
        app:layout_constraintVertical_bias="0.0" />

</androidx.constraintlayout.widget.ConstraintLayout>